-- Initialize Services
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
screenGui.Name = "RattHatterV5Gui"
screenGui.ResetOnSpawn = false

-- Main Frame
local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 600, 0, 700)
mainFrame.Position = UDim2.new(0.5, -300, 0.5, -350)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.BorderColor3 = Color3.fromRGB(139, 0, 0) -- Dark Red Border
mainFrame.BorderSizePixel = 3
mainFrame.Active = true
mainFrame.ClipsDescendants = true

-- Rounded Corners
local corner = Instance.new("UICorner", mainFrame)
corner.CornerRadius = UDim.new(0, 20)

-- Fade-in Effect
mainFrame.BackgroundTransparency = 1
mainFrame.Visible = false
task.spawn(function()
    mainFrame.Visible = true
    for i = 1, 0, -0.05 do
        mainFrame.BackgroundTransparency = i
        task.wait(0.05)
    end
end)

-- Title Label
local titleLabel = Instance.new("TextLabel", mainFrame)
titleLabel.Size = UDim2.new(1, 0, 0, 50)
titleLabel.Position = UDim2.new(0, 0, 0, 10)
titleLabel.BackgroundTransparency = 1
titleLabel.Text = "üêÄ Ratt Hatter"
titleLabel.TextColor3 = Color3.fromRGB(255, 0, 0) -- Hacker Style Red
titleLabel.TextScaled = true
titleLabel.Font = Enum.Font.Code -- Hacker-style code font
titleLabel.Name = "Title"

-- Buttons for Close and Minimize
local closeButton = Instance.new("TextButton", mainFrame)
closeButton.Size = UDim2.new(0, 40, 0, 40)
closeButton.Position = UDim2.new(1, -50, 0, 5)
closeButton.Text = "X"
closeButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.Font = Enum.Font.GothamBold
closeButton.TextScaled = true
local closeCorner = Instance.new("UICorner", closeButton)
closeCorner.CornerRadius = UDim.new(0, 10)

closeButton.MouseButton1Click:Connect(function()
    for i = 0, 1, 0.05 do
        mainFrame.BackgroundTransparency = i
        task.wait(0.05)
    end
    screenGui:Destroy()
end)

local minimizeButton = Instance.new("TextButton", mainFrame)
minimizeButton.Size = UDim2.new(0, 40, 0, 40)
minimizeButton.Position = UDim2.new(1, -100, 0, 5)
minimizeButton.Text = "-"
minimizeButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
minimizeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
minimizeButton.Font = Enum.Font.GothamBold
minimizeButton.TextScaled = true
local minimizeCorner = Instance.new("UICorner", minimizeButton)
minimizeCorner.CornerRadius = UDim.new(0, 10)

local minimized = false
minimizeButton.MouseButton1Click:Connect(function()
    if not minimized then
        for i = mainFrame.Size.Y.Offset, 50, -10 do
            mainFrame.Size = UDim2.new(0, 600, 0, i)
            titleLabel.TextTransparency = i / 700
            task.wait(0.02)
        end
        minimized = true
    else
        for i = 50, 700, 10 do
            mainFrame.Size = UDim2.new(0, 600, 0, i)
            titleLabel.TextTransparency = (700 - i) / 700
            task.wait(0.02)
        end
        minimized = false
    end
end)

-- Scrolling Frame for Buttons
local scrollingFrame = Instance.new("ScrollingFrame", mainFrame)
scrollingFrame.Size = UDim2.new(1, -20, 0, 600)
scrollingFrame.Position = UDim2.new(0, 10, 0, 80)
scrollingFrame.CanvasSize = UDim2.new(0, 0, 2, 0)
scrollingFrame.BackgroundTransparency = 1
scrollingFrame.Name = "ButtonContainer"

-- Add buttons dynamically
local scripts = {
    {"Chat Bypass #1", "loadstring(game:HttpGet('https://pastefy.app/CJ0E33tA/raw'))()"},
    {"Chat Bypass #2", "loadstring(game:HttpGet('https://raw.githubusercontent.com/BakaPraselol/MRCBV4LSB4KRS/main/Loader'))()"},
    -- Add the rest of your buttons here
}

for i, script in ipairs(scripts) do
    local button = Instance.new("TextButton", scrollingFrame)
    button.Size = UDim2.new(0.9, 0, 0, 50)
    button.Position = UDim2.new(0.05, 0, 0, (i - 1) * 60)
    button.Text = script[1]
    button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.Code
    button.TextScaled = true
    button.MouseButton1Click:Connect(function()
        loadstring(script[2])()
    end)

    local btnCorner = Instance.new("UICorner", button)
    btnCorner.CornerRadius = UDim.new(0, 10)
end
